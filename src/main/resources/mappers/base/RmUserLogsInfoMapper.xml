<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.religion.zhiyun.userLogs.dao.RmUserLogsInfoMapper">
    <sql id="logsColumn">
        ID as id,
        USER_NAME as userName,
        USER_NBR as userNbr,
        INTERFACE_CODE as interfaceCode,
        INTERFACE_NAME as interfaceName,
        REQUEST_PARAM as requestParam,
        RESPONSE_RESULT as responseResult,
        FK_DATE as fkDate
    </sql>

    <insert id="addlogs">
        insert into rm_user_logs_info values(#{id},#{userName},#{userNbr},#{interfaceCode},#{interfaceName},
        #{requestParam},#{responseResult},#{fkDate})

    </insert>
    <update id="updatelogs">

    </update>
    <delete id="deletelogs"></delete>

    <select id="alllogs" resultType="com.religion.zhiyun.userLogs.entity.LogsEntity">
        select <include refid="logsColumn"/>
        from rm_user_logs_info
    </select>
</mapper>
